{
  "name": "@prpl/core",
  "version": "0.3.5",
  "description": "HTML-based static site generator",
  "author": "Ty Hopp (https://tyhopp.com)",
  "bin": {
    "prpl": "bin/prpl.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": [
      {
        "require": "./dist/index.cjs",
        "import": "./dist/index.mjs"
      },
      "./dist/index.cjs"
    ],
    "./deno-import-map.json": "./deno-import-map.json"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/packages/core/src/index.d.ts",
  "files": [
    "dist",
    "deno-import-map.json"
  ],
  "keywords": [
    "static-site-generator",
    "ssg",
    "prpl"
  ],
  "license": "MIT",
  "dependencies": {
    "marked": "^4.0.12"
  },
  "devDependencies": {
    "@types/marked": "^4.0.2"
  },
  "gitHead": "57646bee5e16c078b43aa2de487372464594de1c"
}
